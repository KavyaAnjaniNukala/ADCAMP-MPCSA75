class Pattern3
{
   public static void main(String args[ ])
   {
       int i,j;
       char ch='A';
       for(i=0;i<=4;i++)
       {
           for(j=0;j<=i;j++)
            {
               System.out.print(ch+" ");
               ch++;
             }
          System.out.println("");  
       }
    }
}
