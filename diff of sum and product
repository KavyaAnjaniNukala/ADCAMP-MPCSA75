class Solution {
    public int subtractProductAndSum(int n) {
        int sum=0;
        int product=1;
        int a=n;
        while(a>0)
        {
            int r=a%10;
            sum=sum+r;
            product=product*r;
            a=a/10;
        }
          return product-sum;
        
    }
}
